# CarID Local Development Configuration
# Copy this file to .env and fill in your values

# Google Cloud Storage (REQUIRED)
GCS_BUCKET_NAME=your-bucket-name-here
GCS_CREDENTIALS_PATH=/path/to/your/service-account.json

# Optional: Data directory for local development
DATA_DIR=./data

# Model Configuration
DEVICE=cpu
CLIP_MODEL=ViT-B-32
CLIP_PRETRAIN=laion2b_s34b_b79k

# Classification Thresholds
TOPK=5
ACCEPT_THRESHOLD=0.80
MARGIN_THRESHOLD=0.04
NEG_ACCEPT_CAP=0.80

# Other Settings
NEG_PREFIX=_
PROTOTYPE_MODE=true
